def sieve(int n) -> int[]
{
        int[n+1] is_prime;

        for (int i = 0; i <= n; i++)
                is_prime[i] = 1;

        is_prime[0] = 0;
        is_prime[1] = 0;

        for (int p = 2; p * p <= n; p++) {
                if (is_prime[p]) {
                        for (int k = p * p; k <= n; k += p) {
                                is_prime[k] = 0;
                        }
                }
        }

        int[] primes = [];

        for (int i = 2; i <= n; i++) {
                if (is_prime[i])
                        append(primes, i);
        }
        return primes;
}

int n = 100;
println("Calculating primes up to:", n);
int[] p = sieve(n);
println(p);
println("done");
