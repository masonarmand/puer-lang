// Functions returning various types

def return_int() -> int { return 42; }

def return_float() -> float { return 3.14159; }

def return_str() -> str { return "hello, puer!"; }

def return_int_array() -> int[] { return [1, 2, 3, 5, 8, 13]; }

def return_float_array() -> float[] { return [0.1, 0.01, 0.001]; }

def return_str_array() -> str[] { return ["foo", "bar", "baz"]; }

def return_int_2d() -> int[][] { return [[1, 2], [3, 4], [5, 6]]; }

def return_float_2d() -> float[][] { return [[1.1, 2.2], [3.3, 4.4]]; }

def return_str_2d() -> str[][] { return [["a", "b"], ["c", "d"]]; }

println("return_int: ", return_int());
println("return_float: ", return_float());
println("return_str: ", return_str());

println("return_int_array: ", return_int_array());
println("return_float_array: ", return_float_array());
println("return_str_array: ", return_str_array());

println("return_int_2d: ", return_int_2d());
println("return_float_2d: ", return_float_2d());
println("return_str_2d: ", return_str_2d());
